{% extends 'baseAdmin.html.twig' %}

{% block title %}Program{% endblock %}

{% block body %}
<div class="content-wrapper">
    <h1>Program</h1>

    <table id="example1" class="table table-striped">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ program.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ program.name }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_program_index') }}" class="btn btn-info">Retour</a>

    <a href="{{ path('app_program_edit', {'id': program.id}) }}"class="btn btn-success">edit</a>

    {{ include('admin/program/_delete_form.html.twig') }}
</div>    
{% endblock %}
